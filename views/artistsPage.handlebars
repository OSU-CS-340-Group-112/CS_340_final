    <table>
        <thead>
            <th>ArtistID</th>
            <th>Artist Name</th>
            <th>Record Label</th>
        </thead>
        <tbody>
            {{#each artist}}
            <tr>
                <td>{{artistID}}</td>
                <td>{{primaryArtist}}</td>
                <td>{{recordLabel}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>

    <form id="addperson" action="/artists" method="post">
    Artist Name: <input type="text" name="primaryArtist"><br>
    Record Label: <input type="text" name="recordLabel"><br>
    <input type="submit" value="Submit">
    </form>
